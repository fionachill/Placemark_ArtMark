<div class="tile is-ancestor">
    {{#each artmarks}}
        <div class="tile is-parent">
            <article class="tile is-child notification is-dark">
                <p class="title">
                    {{title}}
                </p>
                <p class="subtitle">
                    {{artist}}
                </p>
                <a href="/artmark/{{_id}}" class ="button">
                    <i class="fas fa-monument da-10x"></i>
                </a>
            </article>
        </div>
    {{/each}}
</div>
